{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Launch met lldb",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"server", "start", "--no-daemon", "--port=10338", "--address=localhost"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "refseq",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"refseq", "ncbi-genes-hg19-van-stallman.txt", "--mode=collapse"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		},
		{
			"name": "screen-analyzer map",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"map", "deYtub-10000",
				"hg19",
				"--verbose"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		},
		{
			"name": "screen-analyzer analyze",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"analyze", "PDL1_INFg",
				"hg19", "--reference-hg19=cd274-hg19.txt",
				"--verbose",
				"--mode=collapse",
				"--start=txStart",
				"--end=cdsEnd"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		},
		{
			"name": "screen-analyzer server",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"server", "start", "--no-daemon", "--port=10338", "--address=localhost"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		}

	]
}