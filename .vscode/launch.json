{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Launch met lldb",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"server", "start", "--no-daemon", "--port=10338", "--address=localhost"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "refseq",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"refseq", "--mode=collapse", "--overlap=both", "--start=txStart", "--end=cdsEnd"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		},
		{
			"name": "screen-analyzer map",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"map", "deytub-a",
				"hg19",
				"--verbose"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		},
		{
			"name": "screen-analyzer analyze",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"analyze",
				"acetyltub",
				"--verbose",
				"--mode=collapse",
				"--overlap=both",
				"--start=txStart",
				"--end=cdsEnd"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		},
		{
			"name": "screen-analyzer server",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/screen-analyzer",
			"args": [
				"server", "start", "--no-daemon", "--port=10338", "--address=localhost"
			],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make screen-analyzer"
		}

	]
}